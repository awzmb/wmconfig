{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "vscode-neovim.neovimInitPath": "~/.config/nvim/init.vim",
  "vscode-neovim.neovimPath": "/usr/bin/nvim",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Nord Deep",
  "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#31333c",
      "activityBar.background": "#282a33",
      "editor.background": "#282a33",
      "editor.selectionHighlightBackground": "#31333c",
      "editor.selectionBackground": "#56586b",
      "editor.selectionForeground": "#bbc1ca",
      "editorGroupHeader.tabsBackground": "#282a33",
      "editorGutter.background": "#2c2e38",
      "list.activeSelectionBackground": "#31333c",
      "breadcrumb.background": "#2c2e38",
      "list.hoverBackground": "#31333c",
      "sideBar.background": "#31333c",
      "sideBar.dropBackground": "#282a33",
      "scrollbarSlider.background": "#31333c",
      "statusBar.background": "#282a33",
      "statusBar.border": "#31333c"
  },
  "terminal.integrated.defaultLocation": "editor",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.renderWhitespace": "none",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "git.autofetch": true,
  "go.formatTool": "goimports",
  "java.maven.downloadSources": true,
  "java.jdt.ls.java.home": "/usr/lib/jvm/default-jvm",
  "java.configuration.runtimes": [
    {
      "name": "openjdk11",
      "path": "/usr/lib/jvm/java-11-openjdk"
    }
  ],
  "redhat.telemetry.enabled": false,
  "workbench.startupEditor": "none",
  "groovy.java.home": "/usr/lib/jvm/default-java",
  "workbench.activityBar.visible": false,
  "vim.handleKeys": {
    "<C-k>": false,
    "<C-t>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-h>": false,
    "<C-b>": false,
    "<C-e>": false
  },
  "editor.minimap.enabled": false,
  "editor.showFoldingControls": "mouseover",
  "editor.folding": true,
  "editor.foldingStrategy": "auto",
  "vim.foldfix": true,
  "workbench.editor.enablePreview": false,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "window.menuBarVisibility": "classic",
  "vs-kubernetes": {
    "vscode-kubernetes.kubectl-path.linux": "/root/.local/state/vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/root/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.minikube-show-information-expiration": "2024-03-07T12:48:10.055Z"
  },
  "prettier.printWidth": 160,
  "yaml.format.printWidth": 120,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "java.format.settings.url": "\"java.format.settings.url\": \"https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml\",",
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "Hack",
  "editor.inlayHints.fontFamily": "Hack",
  "scm.inputFontFamily": "Hack",
  "debug.console.fontFamily": "Hack",
  "notebook.output.fontFamily": "Hack",
  "terminal.integrated.fontFamily": "Hack",
  "chat.editor.fontFamily": "Hack"
}
